    },
    "church": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::church.church",
      "inversedBy": "contributions"
    },
    "payment_method": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::payment-method.payment-method"
    },
    "date": {
      "type": "date",
      "required": true
    },
    "reference_number": {
      "type": "string",
      "required": true
    },
    "uuid": {
      "type": "customField",
      "options": {
        "disable-regenerate": true
      },
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "confirmed": {
      "type": "boolean"
    }
