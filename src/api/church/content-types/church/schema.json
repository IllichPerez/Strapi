{
  "kind": "collectionType",
  "collectionName": "churches",
  "info": {
    "singularName": "church",
    "pluralName": "churches",
    "displayName": "churches",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "physical_address": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "members": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "belongs_to_the_church"
    },
    "invitation_id": {
      "type": "customField",
      "options": {
        "uuid-format": "^(?=.*\\d)(?=.*[a-zA-Z])[\\dA-Za-z]{4}-[a-zA-Z\\d]{3}-[\\dA-Za-z]{4}$"
      },
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "church"
    },
    "accepted_payment_methods": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment-method.payment-method"
    },
    "contributions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::contribution.contribution",
      "mappedBy": "church"
    },
    "billing_address": {
      "type": "string",
      "required": false
    },
    "email": {
      "type": "email",
      "required": true
    },
    "ministry_denomination": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "schedules": {
      "type": "text"
    },
    "dogmatic_description": {
      "type": "text"
    },
    "ministries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ministry.ministry",
      "mappedBy": "church"
    },
    "day_off": {
      "type": "enumeration",
      "enum": [
        "lunes",
        "martes",
        "miercoles",
        "jueves",
        "viermes",
        "sabado",
        "domingo"
      ]
    },
    "announcements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::announcement.announcement",
      "mappedBy": "church"
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "church"
    },
    "pastor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "church"
    },
    "uuid": {
      "type": "customField",
      "options": {
        "uuid-format": "^[A-Za-z0-9]{8}$",
        "disable-regenerate": true
      },
      "customField": "plugin::strapi-advanced-uuid.uuid"
    },
    "contribution_purpose": {
      "type": "component",
      "repeatable": true,
      "component": "components.contributions"
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course",
      "mappedBy": "church"
    },
    "prayers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::prayer.prayer",
      "mappedBy": "church"
    },
    "petitions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::petition.petition",
      "mappedBy": "church"
    },
    "mentorships": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mentorship.mentorship",
      "mappedBy": "church"
    }
  }
}
